<?xml version="1.0"?>
<launch>
	<arg name="man" default="manipulator" />
 	<param name="robot_description" command="$(find xacro)/xacro --inorder '$(find man_gazebo)/urdf/$(arg man).urdf.xacro'" />

    <node name="delete_gazebo_model" pkg="rosservice" type="rosservice" args="call /gazebo/delete_model &quot; model_name: 'robot' &quot; "  respawn="false" output="screen"/>


    <!-- push robot_description to factory and spawn robot in gazebo -->
    <node name="spawn_gazebo_model" pkg="gazebo_ros" type="spawn_model" args="-urdf -param robot_description -model robot " respawn="false" output="screen" />

</launch>
